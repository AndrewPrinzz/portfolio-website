{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    // "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "baseUrl": "./src/",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "types": ["node"]
  },
}
